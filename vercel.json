{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/docs",
      "dest": "/index.js"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/404.html"
    }
  ],
  "functions": {
    "index.js": {
      "maxDuration": 30 
    }
  }
}
